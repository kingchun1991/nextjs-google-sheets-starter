    "@emotion/styled": "^11.9.3",
    "@fontsource/lexend": "^4.5.3",
    "csvtojson": "^2.0.10",
    "framer-motion": "^6.3.11",
    "next": "12.1.5",
    "next-pwa": "^5.5.4",
    "next-seo": "^5.4.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.4.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@commitlint/cz-commitlint": "^17.0.0",
    "@types/react": "^18.0.12",
    "commitizen": "^4.2.4",
    "commitlint": "^17.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-sznm": "^1.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "next-sitemap": "^2.5.28",
    "prettier": "^2.6.2",
